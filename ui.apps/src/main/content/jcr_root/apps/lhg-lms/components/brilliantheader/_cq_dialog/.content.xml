<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Properties"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <login
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Log In"
                                        name="./login"/>
                                    <menuitems
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        fieldLabel="Menu Items">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            name="./menuitems">
                                            <items jcr:primaryType="nt:unstructured">
                                                <itemtext
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    emptyText="Item Name"
                                                    fieldLabel="Item Name"
                                                    name="./itemtext"/>
                                                <itemlink
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    emptyText="Item Link"
                                                    fieldLabel="Item Link"
                                                    name="./itemlink"/>
                                            </items>
                                        </field>
                                    </menuitems>
                                    <brilliantimage
                                        jcr:primaryType="nt:unstructured"
                                        sling:orderBefore="login"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Brilliant Logo Image"
                                        name="./brilliantimage"
                                        rootPath="/content/dam/lhg-lms"/>
                                    <loginurl
                                        jcr:primaryType="nt:unstructured"
                                        sling:orderBefore="menuitems"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Log In url"
                                        name="./loginurl"/>
                                    <hamburgericon
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Hamburger Icon Alt Text"
                                        name="./hamburgericon"/>
                                    <createaccountlabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Create Account Label"
                                        name="./createaccountlabel"/>
                                    <createaccountlink
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Create Account Link"
                                        name="./createaccountlink"/>
                                    <createaccounticonalt
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Create Account Icon Alt Text"
                                        name="./createaccounticonalt"/>
                                </items>
                            </column>
                        </items>
                    </properties>
                    <languagetogglenew
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Language Toggle New"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <languagetoggle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        fieldLabel="Language Toggle">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            name="./languagetoggle">
                                            <items jcr:primaryType="nt:unstructured">
                                                <languagelabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Language Label"
                                                    name="./languageLabel"/>
                                                <languagepath
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Language Path"
                                                    name="./languagePath"/>
                                                <languagetitle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Language Title"
                                                    name="./languageTitle"/>
                                                <languageconstant
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Language Constant"
                                                    name="./languageConstant"/>
                                            </items>
                                        </field>
                                    </languagetoggle>
                                </items>
                            </column>
                        </items>
                    </languagetogglenew>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
